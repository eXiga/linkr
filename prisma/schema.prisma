generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Links {
  id         Int        @id @default(autoincrement())
  created_at DateTime?  @default(now()) @db.Timestamptz(6)
  title      String     @db.VarChar(255)
  link       String     @db.VarChar(1000)
  priority   Int
}
